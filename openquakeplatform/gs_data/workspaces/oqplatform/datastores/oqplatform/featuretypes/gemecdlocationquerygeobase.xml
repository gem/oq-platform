  <metadata>
    <entry key="JDBC_VIRTUAL_TABLE">
      <virtualTable>
        <name>gemecdlocationquerygeobase</name>
        <sql>SELECT location.id AS i,location.name AS n,j.%boundarygeom% &#xd;      FROM econd__location AS location &#xd;INNER JOIN %jointable% as j on location.boundaryid = j.%joincolumn% &#xd;     WHERE (parentid = %parentid% or parentid &lt; %override%) &#xd;           AND parenttype=&apos;%parenttype%&apos; &#xd;           AND (location.id = %locationid% OR location.id &lt; %override_i% ) &#xd;           AND j.%boundarygeom% IS NOT NULL &#xd;  ORDER BY j.%joincolumn%</sql>
        <geometry>
          <name>the_geom</name>
          <type>MultiPolygon</type>
          <srid>4326</srid>
        </geometry>
        <parameter>
          <name>jointable</name>
          <defaultValue></defaultValue>
          <regexpValidator>^[\w\d\s]+$</regexpValidator>
        </parameter>
        <parameter>
          <name>override</name>
          <defaultValue>999999999</defaultValue>
          <regexpValidator>^[\w\d\s]+$</regexpValidator>
        </parameter>
        <parameter>
          <name>parentid</name>
          <defaultValue>0</defaultValue>
          <regexpValidator>^[\w\d\s]+$</regexpValidator>
        </parameter>
        <parameter>
          <name>joincolumn</name>
          <defaultValue>gid</defaultValue>
          <regexpValidator>^[\w\d\s]+$</regexpValidator>
        </parameter>
        <parameter>
          <name>locationid</name>
          <defaultValue>0</defaultValue>
          <regexpValidator>^[\w\d\s]+$</regexpValidator>
        </parameter>
        <parameter>
          <name>boundarygeom</name>
          <defaultValue>the_geom</defaultValue>
          <regexpValidator>^[\w\d\s]+$</regexpValidator>
        </parameter>
        <parameter>
          <name>parenttype</name>
          <defaultValue>study</defaultValue>
          <regexpValidator>^[\w\d\s]+$</regexpValidator>
        </parameter>
        <parameter>
          <name>override_i</name>
          <defaultValue>99999999</defaultValue>
          <regexpValidator>^[\w\d\s]+$</regexpValidator>
        </parameter>
      </virtualTable>
    </entry>
