<dataStore>
  <name>oqplatform</name>
  <type>PostGIS</type>
  <enabled>true</enabled>
  <workspace>
    <name>oqplatform</name>
    <atom:link xmlns:atom="http://www.w3.org/2005/Atom" rel="alternate" href="http://127.0.0.1:8080/geoserver/rest/workspaces/oqplatform.xml" type="application/xml"/>
  </workspace>
  <connectionParameters>
    <entry key="port">5432</entry>
    <entry key="schema">public</entry>
    <entry key="passwd">#DB_PASS#</entry>
    <entry key="dbtype">postgis</entry>
    <entry key="host">localhost</entry>
    <entry key="user">#DB_USER#</entry>
    <entry key="database">#DB_NAME#</entry>
    <entry key="namespace">http://oqplatform</entry>
  </connectionParameters>
  <__default>false</__default>
  <featureTypes>
    <atom:link xmlns:atom="http://www.w3.org/2005/Atom" rel="alternate" href="http://127.0.0.1:8080/geoserver/rest/workspaces/oqplatform/datastores/oqplatform/featuretypes.xml" type="application/xml"/>
  </featureTypes>
</dataStore>
