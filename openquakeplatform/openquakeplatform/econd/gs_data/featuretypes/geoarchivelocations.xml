  <metadata>
    <entry key="JDBC_VIRTUAL_TABLE">
      <virtualTable>
        <name>geoarchivelocations</name>
        <sql>SELECT location_geom,locationname AS N,locationid AS I, min(photoid) AS P, count(photoid) AS C &#xd; FROM geoarchivemasteroverview &#xd;WHERE aggregationtest=1 AND location_geom IS NOT NULL&#xd;      AND (eventid = %eventid% OR eventid &lt; %override_e%)&#xd;      AND (inventoryclassid = %inventoryclassid% OR inventoryclassid &lt; %override_i%)&#xd;      AND (unifieddamagelevelid = %damagelevelid% OR unifieddamagelevelid &lt; %override_d%)&#xd;      AND (assetclassid = &apos;%assetclassid%&apos; OR 99999999 = %override_a%)&#xd;GROUP BY location_geom,locationname,locationid</sql>
        <geometry>
          <name>location_geom</name>
          <type>Geometry</type>
          <srid>4326</srid>
        </geometry>
        <parameter>
          <name>inventoryclassid</name>
          <defaultValue>0</defaultValue>
          <regexpValidator>^[\w\d\s]+$</regexpValidator>
        </parameter>
        <parameter>
          <name>override_e</name>
          <defaultValue>99999999</defaultValue>
          <regexpValidator>^[\w\d\s]+$</regexpValidator>
        </parameter>
        <parameter>
          <name>override_a</name>
          <defaultValue>99999999</defaultValue>
          <regexpValidator>^[\w\d\s]+$</regexpValidator>
        </parameter>
        <parameter>
          <name>assetclassid</name>
          <defaultValue>0</defaultValue>
          <regexpValidator>^[\w\d\s]+$</regexpValidator>
        </parameter>
        <parameter>
          <name>damagelevelid</name>
          <defaultValue>0</defaultValue>
          <regexpValidator>^[\w\d\s]+$</regexpValidator>
        </parameter>
        <parameter>
          <name>eventid</name>
          <defaultValue>0</defaultValue>
          <regexpValidator>^[\w\d\s]+$</regexpValidator>
        </parameter>
        <parameter>
          <name>override_i</name>
          <defaultValue>99999999</defaultValue>
          <regexpValidator>^[\w\d\s]+$</regexpValidator>
        </parameter>
        <parameter>
          <name>override_d</name>
          <defaultValue>99999999</defaultValue>
          <regexpValidator>^[\w\d\s]+$</regexpValidator>
        </parameter>
